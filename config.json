{
  "mailApp": {
    "port": 3000,
    "queueInterval": 5000
  },
  "gSender": {
    "queueInterval": 1000,
    "queueSendInterval": 15000
  },
  "gapi": {
    "scopes": [
      "https://mail.google.com/",
      "https://www.googleapis.com/auth/gmail.modify",
      "https://www.googleapis.com/auth/gmail.compose",
      "https://www.googleapis.com/auth/gmail.send"
    ],
    "path": {
      "token": "token.json",
      "credentials": "credentials.json"
    }
  },
  "mongo": {
    "db": "mailappdb",
    "collection": "messages",
    "port": 27017,
    "host": "127.0.0.1"
  },
  "redis": {
    "port": 6379,
    "host": "127.0.0.1"
  }
}